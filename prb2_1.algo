ALGORITHM prb2_1
VAR
    Array1: ARRAY_OF INTEGER[];
    Array2: ARRAY_OF INTEGER[];
    sum: INTEGER := 0 ;
BEGIN
    
    Read(Array1)
    Read(Array2)
    FOR i FROM 0 TO Array1.lenght-1 DO
        FOR j FROM 0 TO Array2.lenght-1 DO
            IF (Array1[i] = Array2[j]) DO
            sum := sum + Array1[i] + Array2[j]
            END_IF
        END_FOR
    END_FOR
    Write(sum)
END